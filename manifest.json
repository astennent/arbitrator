{
  "manifest_version": 2,
  "name": "Arbitrator",
  "version": "0.1",
  "minimum_chrome_version": "29",
  "icons": {
    "128": "app/images/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["chrome/main.js"]
    }
  },
  "file_handlers": {
    "text": {
        "extensions": [
            "arbc"
        ]
    }
  },
  "permissions": [
      {"fileSystem": ["write"]}
  ]
}
